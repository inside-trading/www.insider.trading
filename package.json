{
  "name": "insider-trading-dex",
  "version": "1.0.0",
  "description": "Decentralized Crypto Exchange - Insider Trading",
  "main": "server/index.js",
  "scripts": {
    "dev": "node server/index.js",
    "start": "NODE_ENV=production node server/index.js",
    "dev:frontend": "npx serve public -p 3001",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "deploy:local": "npx hardhat run scripts/deploy.js --network localhost",
    "deploy:sepolia": "npx hardhat run scripts/deploy.js --network sepolia",
    "deploy:holesky": "npx hardhat run scripts/deploy.js --network holesky",
    "node": "npx hardhat node",
    "verify": "npx hardhat verify --network sepolia"
  },
  "keywords": ["dex", "crypto", "exchange", "defi", "web3", "ethereum", "predictions"],
